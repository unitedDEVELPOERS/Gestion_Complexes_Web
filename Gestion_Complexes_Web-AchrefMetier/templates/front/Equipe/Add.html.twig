{% extends'front/base.html.twig' %}
{% block title %} Equipe
{% endblock %}
{% block body %}
    <div class="kode-subheader subheader-height">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h1>Team Detail</h1>
                </div>
                <div class="col-md-6">
                    <ul class="kode-breadcrumb">
                        <li><a href="{{ asset('/rechercherequipe') }}">Rechercher Equipe</a></li>
                        <li><a href="{{ asset('/equipe/Add') }}">Ajouter Equipe</a></li>
                        <li><a href="{{ asset('/calender') }}">List des Match</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!--// SubHeader //-->

    <!--// Main Content //-->
    <div class="kode-content">

        <!--// Page Content //-->
        <section class="kode-pagesection">
            <div class="container">
                <div class="row">

                    <div class="kode-pagecontent col-md-9">



                        <div class="team-detail-table-ap padding-bottom-30-flat">

                        <!--Team Members -->


                        <div id="koderespond">
                            <div class="kode-section-title"> <h2>Ajout Equipe</h2> </div>

                                  {{ form(formequipe, {'attr': {'novalidate': 'novalidate'}}) }}




                        </div>

                    </div>


                </div>
            </div>
        </section>
        <!--// Page Content //-->

    </div>
    <!--// Main Content //-->

    <!--// NewsLatter //-->
    <div class="kode-newslatter kode-bg-color" >
        <span class="kode-halfbg thbg-color"></span>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h3>Subcribe Our Monthly Newsletter</h3>
                </div>
                <div class="col-md-6">
                    <form>
                        <input type="text" placeholder="Your E-mail Adress" name="s" required>
                        <label><input type="submit" value=""></label>
                    </form>
                </div>
            </div>
        </div>
    </div>
{% endblock %}