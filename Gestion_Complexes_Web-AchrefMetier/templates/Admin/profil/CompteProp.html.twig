{% extends'Admin/index.html.twig' %}
{% block title %} Utilisateur
{% endblock %}
{% block body %}

    <body class="">
    {% for message in app.flashes('success') %}
        <div class="alert alert-success mt-2">
            {{ message }}
        </div>
    {% endfor %}
    {% for message in app.flashes('danger') %}
        <div class="alert alert-danger mt-2">
            {{ message }}
        </div>
    {% endfor %}
    <h1>Liste des utilisateur</h1>
    <div class="float-right" style="margin-right: 90px">

    </div><br>
    <form role="form" class="form-horizontal">

        <div class="form-group">
            <label class="col-lg-2 control-label"> Email</label>
            <div class="col-lg-6">
                {{ Utilisateur.email }}
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-2 control-label"> Matricule fiscale</label>
            <div class="col-lg-6">
                {{ Utilisateur.matriculeFiscale }}

            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-2 control-label"> Raison Sociale</label>
            <div class="col-lg-6">

                {{ Utilisateur.raisonSociale }}
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-2 control-label"> TELEPHONE</label>
            <div class="col-lg-6">
                {{ Utilisateur.telephone }}
            </div>
        </div>


    </form>
    <tr>
        <td><a href="{{ path('updateProp',{'id':Utilisateur.id}) }}"> Update</a> </td>
    </tr>



    </body>
{% endblock %}