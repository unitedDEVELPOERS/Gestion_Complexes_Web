<body>
<H1>Liste des equipe </H1>
<table border="1">
    <tr>
        <th>ID</th>
        <th>NOM</th>
        <th>NBR</th>
        <td>DELETE</td>


    </tr>
    <tr{% for c in Equipe%}>
        <td>{{ c.id }}</td>
        <td>{{ c.nom}}</td>
        <td>{{ c.nbreJoueur }}</td>
        <td><a href="{{ path('updateequipe',{'id':c.id}) }}">Update</a> </td>
        <td><a href="{{ path('deleteequipe',{'id':c.id}) }}">Delete</a> </td>

    </tr{% endfor %}>
</table>

</body>
