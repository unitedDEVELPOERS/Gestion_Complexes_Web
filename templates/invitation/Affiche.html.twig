<H1>Liste des Invitation </H1>
<table border="1">
    <tr>
        <th>ID</th>
        <th>TYPE</th>



    </tr>
    <tr{% for c in Invitation %}>
        <td>{{ c.id }}</td>
        <td>{{ c.type}}</td>

        <td><a href="{{ path('updateinvitation',{'id':c.id}) }}">Update</a> </td>
